{
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.8",
    "@types/nodemailer": "^6.4.4",
    "ejs-lint": "^1.2.1",
    "jest": "^27.4.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/mssql": "^7.1.4",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/validator": "^13.7.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "mssql": "^7.3.0",
    "nodemailer": "^6.7.2",
    "nodemon": "^2.0.15",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "tscpaths": "^0.0.9",
    "validator": "^13.7.0"
  },
  "main": "src/index.ts",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/app.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "start": "nodemon",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "debug": "node -r tsconfig-paths/register -r ts-node/register ./src/debug.ts",
    "jest": "jest"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  }
}
